$(document).ready(function(){
	$('div.section').click(function(){
		val = $(this).attr('ref');
		if($('div.console').children("div[style='display: block;']").size() > 0){
		    $('div.console').children("div[style='display: block;']").fadeOut('slow',function(){
			    $('#'+val).fadeIn('slow');
			});
		}
		else{
		    $('#'+val).fadeIn('slow');
		}
	    });
});